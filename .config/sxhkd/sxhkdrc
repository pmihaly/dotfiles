# gui programs
super + {w, i, C}
    {firefox, dmenuunicode, dmenuqalc}

# terminal programs
super + {a, r, v, p, g}
    $TERMINAL -e {pulsemixer, $FILEBROWSER, $EDITOR -c VimwikiIndex, projectfzf, tuxifzf}

super + e
    mw -Y & $TERMINAL -e neomutt

# content consooming
super + {n, y, J, S}
    $TERMINAL -e {newsboat -c ~/sync/newsboat-cache, ytfzf -lt, jftui, streamfzf}

# dotfiles obsession
super + {c, s}
    $TERMINAL -e {configfzf, scriptfzf}

# system
super + {W, x, X}
    {dmenuwifi, slock, poweroff}

Print
    scrot
